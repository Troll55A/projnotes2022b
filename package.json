  "scripts": {
    "start": "node ./bin/www",
    "devc": "NODE_ENV=develoment && webpack serve --config ./webpack.dev.config.js --mode development",
    "dev": "nodemon ./Server/bin/www",
    "devb": "babel-watch ./Server/bin/www",
    "cleans": "rm -rf dist",
    "cleanc": "rm -f bundle.js ./stylesheets/app.css",
